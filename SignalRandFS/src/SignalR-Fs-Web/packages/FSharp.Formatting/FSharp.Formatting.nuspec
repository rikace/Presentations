  <metadata>
    <id>FSharp.Formatting</id>
    <version>2.6.3</version>
    <authors>Tomas Petricek, Oleg Pestov, Anh-Dung Phan, Xiang Zhang</authors>
    <owners>Tomas Petricek, Oleg Pestov, Anh-Dung Phan, Xiang Zhang</owners>
